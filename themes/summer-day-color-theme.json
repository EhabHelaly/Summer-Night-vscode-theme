| Author	: 	Mohamed Ehab Helaly											|
| Comments	: 	Theme is basicly for (C/C++, XML, OIL, LUA) syntax highlight|
\*=========================================================================*/
{
	"colors": {
		"activityBar.activeBackground": "#305070",
		"activityBar.activeBorder": "#305070",
		"activityBarBadge.background": "#007acc",
		"breadcrumb.background": "#eeeeee",
		"breadcrumb.foreground": "#000000",
		"contrastBorder": "#aaaaaa",
		"contrastActiveBorder": "#aaccee",
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"editor.inactiveSelectionBackground": "#e5ebf1",
		"editor.selectionHighlightBackground": "#add6ff80",
		"editorGroupHeader.tabsBackground": "#eeeeee",
		"editorIndentGuide.activeBackground": "#939393",
		"editorIndentGuide.background": "#d3d3d3",
		"editorSuggestWidget.background": "#f3f3f3",
		"foreground": "#000000",
		"input.border": "#aaaaaa",
		"input.placeholderForeground": "#767676",
		"list.hoverBackground": "#e8e8e8",
		"panel.background": "#eeeeee",
		"panel.border": "#aaaaaa",
		"panelTitle.activeForeground": "#000000",
		"panelTitle.inactiveForeground": "#555555",
		"settings.numberInputBorder": "#cecece",
		"settings.textInputBorder": "#cecece",
		"sideBar.background": "#eeeeee",
		"sideBarSectionHeader.background": "#aaccee",
		"sideBarTitle.foreground": "#000000",
		"statusBarItem.remoteBackground": "#16825d",
		"statusBarItem.remoteForeground": "#ffffff",
		"tab.activeForeground": "#000000",
		"tab.border": "#cccccc",
		"tab.activeBackground": "#aaccee",
		"tab.inactiveBackground": "#eeeeee",
		"tab.inactiveForeground": "#000000",
		"terminal.background": "#ffffdd",
		"terminal.foreground": "#000000",
	},
	"tokenColors": [
		{
			"settings": {
				"background": "#FFFFFF",
				"foreground": "#08080e"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"background": "#FFFFFF",
				"foreground": "#08080e"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#579b91"
			}
		},
		{
			"name": "Comment doxyen",
			"scope": "storage.type.class.doxygen.c",
			"settings": {
				"foreground": "#6bbeb1"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#995c00"
			}
		},
		{
			"name": "Template Definition",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#a64242"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#080808"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ef4c9c"
			}
		},
		{
			"name": "Number suffix/prefix",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#e2a6c3"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language, support.constant",
			"settings": {
				"foreground": "#1e824c"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#acac00"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#080808"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#1c00c4"
			}
		},
		{
			"name": "Preprocessor",
			"scope": "keyword.control.directive, meta.preprocessor",
			"settings": {
				"foreground": "#00AA00"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#1c00c4"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.function.declaration, entity.name.scope-resolution",
			"settings": {
				"fontStyle": "",
				"foreground": "#ad00c4"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ad00c4"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#1b2b86"
			}
		},
		{
			"name": "Macro",
			"scope": "entity.name.function.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#1e824c"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#990033"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#1b2b86"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#1b7a12"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#208181"
			}
		},
		{
			"name": "Library class/type",
			"scope": "support.type, support.class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ad00c4"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#a64242",
				"fontStyle": "",
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#AE81FF",
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "JSON String",
			"scope": "meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#472f00"
			}
		},
		{
			"name": "diff.header",
			"scope": "meta.diff, meta.diff.header",
			"settings": {
				"foreground": "#aabbaa"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#e40303"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#aaff00"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#e69500"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#7936f7a0"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#999900"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#a64242"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#999900"
			}
		},
		{
			"name": "Markup Styling",
			"scope": "markup.bold, markup.italic",
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#FD971F"
			}
		},
		{
			"name": "Markup Headings",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"fontStyle": "",
				"foreground": "#A6E22E"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language, variable.object",
			"settings": {
				"foreground": "#933401"
			}
		},
		{
			"name": "Punctuations",
			"scope": "punctuation.terminator, punctuation.separator, keyword.operator.separator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f4000c"
			}
		},
		{
			"name": "Brackets",
			"scope": "punctuation.section",
			"settings": {
				"foreground": "#f4000c"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator, punctuation.accessor",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f4000c"
			}
		}
	]
}
